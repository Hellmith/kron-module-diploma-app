{	"name": "client-side",	"$schema": "../../node_modules/nx/schemas/project-schema.json",	"sourceRoot": "apps/client-side/src",	"projectType": "application",	"targets": {		"build": {			"executor": "@nrwl/vite:build",			"outputs": ["{options.outputPath}"],			"defaultConfiguration": "production",			"options": {				"outputPath": "dist/apps/client-side"			},			"configurations": {				"development": {					"mode": "development"				},				"production": {					"mode": "production"				}			}		},		"serve": {			"executor": "@nrwl/vite:dev-server",			"defaultConfiguration": "development",			"options": {				"buildTarget": "client-side:build"			},			"configurations": {				"development": {					"buildTarget": "client-side:build:development",					"hmr": true				},				"production": {					"buildTarget": "client-side:build:production",					"hmr": false				}			}		},		"preview": {			"executor": "@nrwl/vite:preview-server",			"defaultConfiguration": "development",			"options": {				"buildTarget": "client-side:build"			},			"configurations": {				"development": {					"buildTarget": "client-side:build:development"				},				"production": {					"buildTarget": "client-side:build:production"				}			}		},		"test": {			"executor": "@nrwl/vite:test",			"outputs": ["coverage/apps/client-side"],			"options": {				"passWithNoTests": true,				"reportsDirectory": "../../coverage/apps/client-side"			}		},		"lint": {			"executor": "@nrwl/linter:eslint",			"outputs": ["{options.outputFile}"],			"options": {				"lintFilePatterns": ["apps/client-side/**/*.{ts,js,vue}"]			}		}	},	"tags": ["t"]}